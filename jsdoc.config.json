{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["index.js", "actions"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
