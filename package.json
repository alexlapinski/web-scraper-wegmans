{
  "name": "web-scraper-wegmans",
  "version": "1.0.0",
  "description": "A web scraper for Wegman's receipt data.",
  "main": "index.js",
  "repository": "git@gitlab.com:lapinski/web-scraper.wegmans.git",
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "debug": "ts-node ./src/index.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json -t codeFrame",
    "lint-fix": "npm run lint -- --fix",
    "prebuild": "rm -rf dist/",
    "build": "npm run lint && npm run build-ts",
    "build-ts": "tsc",
    "test": "jest",
    "test:unit": "mocha --require ts-node/register test/unit/**/*.ts",
    "test:integration": "mocha --require ts-node/register test/integration/**/*.ts",
    "test:coverage": "nyc npm run test",
    "predb": "npm run build",
    "db": "typeorm",
    "db:generate": "npm run db -- migration:generate",
    "db:migrate": "npm run db -- migration:run",
    "db:revert": "npm run db --  migration:revert"
  },
  "keywords": [
    "web-scraper",
    "wegmans"
  ],
  "author": "Alex Lapinski <contact@alexlapinski.name>",
  "license": "MIT",
  "dependencies": {
    "convict": "^4.4.0",
    "dotenv": "^6.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pg": "^7.7.1",
    "puppeteer": "^1.4.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/convict": "^4.2.1",
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.119",
    "@types/moment": "^2.13.0",
    "@types/puppeteer": "^1.11.1",
    "@types/winston": "^2.4.4",
    "jest": "^23.6.0",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "substitute.js": "^1.0.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
