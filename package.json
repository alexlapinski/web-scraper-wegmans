{
  "name": "web-scraper-wegmans",
  "version": "1.0.0",
  "description": "A web scraper for wegmans receipt data.",
  "main": "index.js",
  "repository": "git@gitlab.com:alex.lapinski/web-scraper-wegmans.git",
  "scripts": {
    "start": "node ./index.js",
    "lint": "eslint **/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "docs": "jsdoc -P ./package.json -c ./jsdoc.config.json -R ./README.md -d ./docs/ ."
  },
  "keywords": [
    "web-scraper",
    "wegmans"
  ],
  "author": "Alex Lapinski <contact@alexlapinski.name>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "envalid": "^4.1.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "puppeteer": "^1.4.0",
    "sequelize": "^4.37.10",
    "sqlite3": "^4.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "prettier": "1.13.5",
    "sequelize-cli": "^4.0.0"
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ]
  }
}
