{
  "name": "web-scraper-wegmans",
  "version": "1.0.0",
  "description": "A web scraper for wegmans receipt data.",
  "main": "index.js",
  "repository": "git@gitlab.com:alex.lapinski/web-scraper-wegmans.git",
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "debug": "ts-node ./src/index.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json -t codeFrame",
    "lint-fix": "npm run lint -- --fix",
    "build": "npm run lint && npm run build-ts",
    "build-ts": "tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test:coverage": "nyc npm run test",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [
    "web-scraper",
    "wegmans"
  ],
  "author": "Alex Lapinski <contact@alexlapinski.name>",
  "license": "MIT",
  "dependencies": {
    "convict": "^4.4.0",
    "dotenv": "^6.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pg": "^7.7.1",
    "puppeteer": "^1.4.0",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.0",
    "typeorm": "^0.2.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/convict": "^4.2.1",
    "@types/dotenv": "^6.1.0",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/puppeteer": "^1.11.1",
    "@types/sequelize": "^4.27.33",
    "@types/sqlite3": "^3.1.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
